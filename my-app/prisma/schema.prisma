generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Konsultasi {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())

  nama      String
  umur      Int
  alamat    String   @db.Text

  jawaban   Json
  hasil     Json
}

model Admin {
  id        String   @id @default(cuid())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
}